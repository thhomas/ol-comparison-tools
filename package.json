{
  "name": "ol-comparison-tools",
  "version": "1.0.0",
  "description": "An Openlayers extension adding a control to handle comparison tools between 2 layers.",
  "main": "comparisonTools.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "serve": "http-server",
    "test": "phantomjs --web-security=false --ssl-protocol=any ./node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js test/TestRunner.html spec",
    "lint": "eslint ./src/**/*.js --ext .js src test --cache",
    "build": "gulp build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thhomas/ol-comparison-tools.git"
  },
  "keywords": [
    "openlayers",
    "ol3-ext",
    "swipe",
    "clip"
  ],
  "author": "Thomas Tilak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thhomas/ol-comparison-tools/issues"
  },
  "homepage": "https://github.com/thhomas/ol-comparison-tools#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "jquery": "^3.2.1",
    "ol": "^4.6.4",
    "ol-ext": "git+https://github.com/thhomas/ol-ext.git"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "del": "^3.0.0",
    "gulp-concat-util": "^0.5.5",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "http-server": "^0.10.0",
    "mocha": "^3.5.0",
    "mocha-phantomjs-core": "^2.1.2"
  }
}
